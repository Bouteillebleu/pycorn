OBJECTS = main.o getpath.o frozen.o $(EXTMODULES) $(ROOT)/python/libpython2.5.a
SYSLIBS := -lm
RAMDISK := initrd.zip

$(phony all): pykern.flash

include pyinclude.mk
include common.mk

