	python freeze.py $(output) $(FREEZEDIRS) $(ROOT)/python/frozen

initrd.zip: $( $(INITRDDIRS))/** $(ROOT)/python/stamp-initrd makezip.py
	python makezip.py $(output) $(INITRDDIRS) $(ROOT)/python/initrd
